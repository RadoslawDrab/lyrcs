{
  "include": [
	"src/**/*"
  ],
  "compilerOptions": {
	"target": "ES2023",
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"strict": false,
	"skipLibCheck": true,
	"baseUrl": "./src",
	"paths": {
	  "@utils/*": [
		"utils/*"
	  ],
	  "@type/*": [
		"types/*"
	  ],
	  "@components/*": [
		"components/*"
	  ],
	  "@assets/*": [
		"assets/*"
	  ]
	}
  }
}
